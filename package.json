{
  "name": "hdcx",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "export NODE_OPTIONS=--max-old-space-size=16000 && docusaurus build",
    "dev": "set NODE_OPTIONS=--max-old-space-size=16000 && docusaurus start --host 0.0.0.0 --no-open",
    "build:win": "set NODE_OPTIONS=--max-old-space-size=16000 && docusaurus build --config dev.config.js",
    "check": "npx docusaurus-mdx-checker",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "^3.6.3",
    "@docusaurus/preset-classic": "^3.6.3",
    "@gera2ld/jsx-dom": "^2.2.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "use-local-storage-state": "^19.4.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.6.3",
    "esbuild-loader": "^4.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}